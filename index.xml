<Application>
	<Build>
		<task action="skip" source="src/js/xlsx/main.js" destination="public/js/xdef.js"/>
		<task action="skip" source="src/js/bundle.js" destination="public/js/bundle.min.js"/>
	</Build>
	<Head>
		<meta name="id" value="eniac"/>
		<meta name="title" value="Eniac" version="1.2"/>
		<meta name="author" value="Hakan Bilgin" namespace="ant"/>
		<meta name="license" value="AGPL-3.0-or-later"/>
		<meta name="repository" value="https://github.com/Defiant-System/eniac"/>
		<meta name="width" value="1180" min="720"/>
		<meta name="height" value="780" min="420"/>
		<meta name="top" value="90"/>
		<meta name="left" value="70"/>
		<meta name="minimize"/>
		<meta name="maximize"/>
		<meta name="resize"/>
		<meta name="script" value="src/js/main.js"/>
		<meta name="style" value="src/css/main.less"/>
		<meta name="icons" value="src/svg/icons.svg"/>
		<meta name="content" value="src/partials/content.htm"/>
		<meta name="xsl" value="src/partials/templates.xsl"/>
		
		@import "src/partials/data.xml"
		@import "src/partials/fn-info.xml"
		@import "src/partials/toolbar.xml"
		@import "src/partials/menubar.xml"
	</Head>
</Application>