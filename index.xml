<Application>
	<Build>
		<task action="skip" source="src/js/external/xlsx.js" destination="public/js/xlsx.js"/>
	</Build>
	<Head>
		<meta name="id" value="eniac"/>
		<meta name="title" value="Eniac" version="1.0"/>
		<meta name="author" value="Hakan Bilgin" namespace="ant"/>
		<meta name="license" value="AGPL-3.0-or-later"/>
		<meta name="repository" value="https://github.com/Defiant-System/eniac"/>
		<meta name="width" value="800" min="720"/>
		<meta name="height" value="500" min="420"/>
		<meta name="top" value="180"/>
